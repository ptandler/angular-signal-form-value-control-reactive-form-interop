{
  "trailingComma": "all",
  "singleQuote": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "printWidth": 80,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@etalytics/(.*)$",
    "^@eta/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
